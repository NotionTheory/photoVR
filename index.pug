doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="origin-trial", content=GoogleWebVROriginTrialToken)
    title #{description}
    if debug
        script(src= fileRoot + "node_modules/pliny/pliny" + jsExt)
        script(src= fileRoot + "../Primrose/PrimroseDependencies" + jsExt)
        script(src= fileRoot + "../Primrose/PrimroseLib-WithDoc" + jsExt)
    else
        script(src= "https://primrosevr.com/archive/Primrose-0.26.24.min.js")
    link(type="text/css", rel="stylesheet", href= fileRoot + "style" + cssExt)
  body
    canvas#frontBuffer
    #fsb
    a(href="https://github.com/capnmidnight/photoVR")
        img(style="position: absolute; top: 0; right: 0; border: 0;", src="https://camo.githubusercontent.com/652c5b9acfaddf3a9c326fa6bde407b87f7be0f4/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f6f72616e67655f6666373630302e706e67", alt="Fork me on GitHub", data-canonical-src="https://s3.amazonaws.com/github/ribbons/forkme_right_orange_ff7600.png")
    script(src=fileRoot + packageName + jsExt)